{
  "name": "jest-serializer-graphql-schema",
  "version": "1.0.0",
  "description": "Serialize GraphQL schemas for Jest snapshot tests",
  "main": "lib/index.js",
  "author": "Ryan Elliott-Potter <ryan@ryanep.com>",
  "license": "MIT",
  "keywords": [
    "jest",
    "snapshot",
    "serializer",
    "graphql",
    "schema",
