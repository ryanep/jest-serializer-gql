{
  "name": "jest-serializer-gql",
  "description": "Serialize GraphQL schemas for Jest snapshot tests",
  "version": "0.1.0",
  "author": "Ryan Elliott-Potter <ryan@ryanep.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanep/jest-serializer-gql.git"
  },
  "bugs": {
    "url": "https://github.com/ryanep/jest-serializer-gql/issues"
  },
  "keywords": [
    "jest",
    "snapshot",
    "serializer",
    "graphql",
    "schema",
    "unit",
    "test"
