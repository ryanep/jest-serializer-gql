{
  "name": "jest-serializer-gql",
  "version": "1.2.0",
  "description": "Serialize GraphQL schemas for Jest snapshot tests",
  "keywords": [
    "jest",
    "snapshot",
    "serializer",
    "graphql",
    "schema",
    "unit",
    "test"
  ],
  "bugs": {
    "url": "https://github.com/ryanep/jest-serializer-gql/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanep/jest-serializer-gql.git"
  },
  "license": "MIT",
  "author": "Ryan Elliott-Potter <ryan@ryanep.com>",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./lib",
    "lint": "eslint './**/*.ts'",
    "fmt": "prettier --write './**/*.ts'",
    "test": "jest --config=jest.config.json",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/npm": "^7.1.1",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "alex": "^9.1.0",
    "codecov": "^3.8.1",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "semantic-release": "^19.0.3",
    "sort-package-json": "^1.49.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.4"
  },
